{
  "name": "islamalsayed/itoast",
  "description": "Flexible and animated toast notification system for Laravel",
  "keywords": [
    "laravel",
    "itoast",
    "toast",
    "confirm",
    "notification",
    "alert",
    "frontend"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "IslamAlsayed",
      "email": "eslamalsayed8133@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "archive": {
    "exclude": [
      "demo"
    ]
  },
  "autoload": {
    "psr-4": {
      "IslamAlsayed\\Itoast\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "IslamAlsayed\\Itoast\\IToastServiceProvider"
      ],
      "aliases": {
        "IToast": "IslamAlsayed\\Itoast\\Facades\\IToastFacade"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "source": "https://github.com/IslamAlsayed/itoast-and-confirm-package"
  }
}
